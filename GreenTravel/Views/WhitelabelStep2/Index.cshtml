@model IEnumerable<GreenTravel.Models.WhitelabelStep2>

@{
   
    ViewBag.Title = "Comapany Application Features";
    ViewBag.PageTitle = "Comapany Application Features";
}
@{ 
    var queryValue = Request.QueryString["id"];  
}



<link href="/assets/plugins/gridgallery/css/component.css" rel="stylesheet" type="text/css" />
<style>
    .tab-itenaries > .col-md-1 > h4,
    .tab-itenaries .col-md-2 > h4,
    .tab-itenaries .col-md-3 > h4,
    .tab-itenaries .col-md-4 > h4,
    .tab-itenaries .col-md-5 > h4,
    .tab-itenaries > .col-md-6 > h4,
    .tab-itenaries .col-md-7 > h4,
    .tab-itenaries .col-md-8 > h4,
    .tab-itenaries .col-md-9 > h4,
    .m-b-lg > h4 {
        border-bottom: 1px dotted #ccc;
        color: #40474f;
        font-size: 22px;
        font-weight: normal;
        margin: 7px 0 10px;
        padding-bottom: 5px;
    }

    .tab-itenaries .col-md-1 > h5,
    .tab-itenaries .col-md-2 > h5,
    .tab-itenaries .col-md-3 > h5,
    .tab-itenaries .col-md-4 > h5,
    .tab-itenaries .col-md-5 > h5,
    .tab-itenaries .col-md-6 > h5,
    .tab-itenaries .col-md-7 > h5,
    .tab-itenaries .col-md-8 > h5,
    .tab-itenaries .col-md-9 > h5,
    .m-b-lg > h5 {
        /*color: #40474f;*/
        font-size: 19px;
        font-weight: bold;
        margin: 0 0 10px;
        padding-bottom: 5px;
        border-bottom: 1px dotted;
        text-shadow: 1px 0px #BEC8D2;
    }

        .tab-itenaries .col-md-1 > h5 > span,
        .tab-itenaries .col-md-2 > h5 > span,
        .tab-itenaries .col-md-3 > h5 > span,
        .tab-itenaries .col-md-4 > h5 > span,
        .tab-itenaries .col-md-5 > h5 > span,
        .tab-itenaries .col-md-6 > h5 > span,
        .tab-itenaries .col-md-7 > h5 > span,
        .tab-itenaries .col-md-8 > h5 > span,
        .tab-itenaries .col-md-9 > h5 > span,
        .m-b-lg > h5 > span {
            float: right;
        }

    .tab-itenaries a > span {
        font-weight: bold;
        font-size: 14px;
    }

    /*.tab-itenaries span {
        font-weight: bold;
        font-size: 14px;
        text-shadow: 1px 0 #BEC8D2;
    }*/

    .tab-itenaries {
        padding-bottom: 10px;
        border: 1px double #BEC8D2 !important;
        border-radius: 5px !important;
        box-shadow: 3px 3px 7px -5px rgba(0, 0, 0, 0.5);
        /*margin-bottom: 15px;*/
    }

        .tab-itenaries .form-control {
            border-width: 0;
            padding: 3px 10px !important;
        }

            .tab-itenaries .form-control .btn, .tab-itenaries .form-control .label {
                width: 100%;
            }

            .tab-itenaries .form-control h2 {
                margin-bottom: 0;
                margin-top: 0;
            }

        .tab-itenaries .right-border {
            border-right: 1px dotted #f25656;
            min-height: 156px;
            padding-left: 25px;
        }

        .tab-itenaries .form-control .btn {
            padding: 8px 10px;
            font-size: 13px;
            text-transform: uppercase;
        }

        .tab-itenaries span.total-cost {
            font-size: 11px;
        }

        .tab-itenaries .table > tbody > tr:first-child > td {
            border-width: 0;
        }

        .tab-itenaries .table > tbody > tr > td:first-child {
            width: 60%;
        }

        .tab-itenaries .table > tbody > tr > td:last-child {
            text-align: right;
            width: 40%;
        }

        .tab-itenaries .table > tbody > tr > td {
            font-size: 11px;
            padding: 7px !important;
        }

    .grid figcaption {
        background: #fff none repeat scroll 0 0;
        padding: 25px;
    }

    .grid li:hover figure {
        opacity: 1;
    }

    .grid li {
        cursor: default;
    }

    figcaption > h3 {
        border-bottom: 1px dashed #3c3c3c !important;
    }
</style>

@using (Html.BeginForm())
{
    <div class="col-md-12 form-detail">
        <div class="panel panel-white">
            <div class="panel-body">
                <div role="tabpanel">
                    <!-- Nav tabs -->
                    <div class="tab-content">
                        <div role="tabpanel" class="tab-pane active">
                            <form>
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label>Feature Category </label>
                                            <select id="drpFeatureCategory" class="js-states form-control" style="display: none; width: 100%">
                                            </select>
                                            <input type="text" class="form-control" style="display:none"  id="txtCorporateID" value="@queryValue" readonly="true">
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div id="grid-gallery" class="grid-gallery">
                                        <section class="grid-wrap">
                                            <ul class="grid">
                                                <li class="grid-sizer"></li>
                                                <div id="partial"></div>
                                            </ul>
                                        </section>
                                        <section class="slideshow">
                                            <ul>
                                                <li></li>
                                            </ul>
                                        </section>
                                    </div>


                                </div>
                                <div class="col-md-12">
                                    <form>
                                        <br />
                                        <button id="btnSave" type="submit" class="btn btn-success btnSaveStep2">
                                            SAVE</button>
                                        <button id="btnUpdate" type="submit" class="btn btn-primary btnSaveStep2">
                                            UPDATE</button>
                                        <button id="btnClear" type="submit" class="btn btn-warning ">
                                            CLEAR</button>
                                        <button id="btnQuit" type="submit" class="btn btn-info ">
                                            QUIT FORM</button>
                                    </form>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}

@section Scripts{
    <script src="/assets/plugins/gridgallery/js/imagesloaded.pkgd.min.js"></script>
    <script src="/assets/plugins/gridgallery/js/masonry.pkgd.min.js"></script>
    <script src="/assets/plugins/gridgallery/js/classie.js"></script>
    <script src="/assets/plugins/gridgallery/js/cbpgridgallery.js"></script>
    <script src="/assets/js/pages/gallery.js"></script>

    <script src="~/Scripts/JS/Page/WhitelabelStep2.js"></script>


}