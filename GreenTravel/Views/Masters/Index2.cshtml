                        <input type="text" class="form-control" id="Textbox4" placeholder="User Unique Code">
                    </div>
                    <div class="form-group">
                        <label for="txtSrNo">Textbox 5</label>
                        <input type="text" class="form-control" id="Textbox5" placeholder="User Unique Code">
                    </div>

                    <div class="form-group">
                        <label for="txtLastName">Rating 1</label>
                        <select id="Rating1" class="js-states form-control" tabindex="-1" style="display: none; width: 100%">
                            <option value="1">1</option>
                            <option value="2">2</option>
                            <option value="3">3</option>
                            <option value="4">4</option>
                            <option value="5">5</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="txtLastName">Rating 2</label>
