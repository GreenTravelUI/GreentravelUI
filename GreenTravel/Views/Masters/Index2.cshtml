@{
    ViewBag.Title = "Masters";
    ViewBag.PageTitle = "Masters";
    ViewBag.FooterCopyRight = "2015 &copy; Modern by Steelcoders.";
}
<div class="col-md-12 form-detail">
    <div class="panel panel-white">
        <div class="panel-body">
            <div class="col-md-7">
                <form>
                    <div class="form-group">
                        <label for="txtSrNo">Sr. No.</label>
                        <input type="text" class="form-control" id="txtSrNo" placeholder="User Unique Code" readonly>
                    </div>
                    <div class="form-group">
                        <label for="txtDescription">
                            Description 
                        </label>
                        <textarea cols="5" class="form-control" id="txtDescription"></textarea>
                    </div>
                    <div class="form-group">
                        <label for="txtCode">Code </label>
                        <input type="text" class="form-control" id="txtCode" placeholder="First Name">
                    </div>
                    <div class="form-group">
                        <label for="txtLastName">Active </label>
                        <select id="drpTitle" class="js-states form-control" tabindex="-1" style="display: none; width: 100%">
                            <option value="1">Active</option>
                            <option value="0">InActive</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="txtDescription">
                            Remarks 
                        </label>
                        <textarea cols="5" class="form-control" id="txtRemarsk"></textarea>
                    </div>
                    <div class="form-group">
                        <label for="txtLastName">Dropdown 1</label>
                        <select id="Dropdown1" class="js-states form-control" tabindex="-1" style="display: none; width: 100%">
