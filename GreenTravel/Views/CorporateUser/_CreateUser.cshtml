<div class="col-md-8 full-responsive">
    <form>
        <div class="form-group formcorporatedetail">
            <div class="row">
                <div class="col-md-12">
                    <label>Corporate <span>*</span></label>
                    <select id="drpCorporate" class="js-states col-sm-2 form-control Dropdown formcorporate" style="display: none; width: 100%"></select>
                </div>

            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-md-6">
                    <label>Unit <span>*</span></label>
                    <select id="DrpUnitTab2" class="js-states col-sm-2  form-control Dropdown " style="display: none; width: 100%"></select>
                </div>
                <div class="col-md-6">
                    <label>Location <span>*</span></label>
                    <select id="DrpLocationTab2" class="js-states col-sm-2 form-control Dropdown " style="display: none; width: 100%"></select>
                </div>

            </div>
        </div>
        <div class="form-group">
            <input type="text" class="form-control col-sm-5 textbox-control" id="txtsrno" placeholder="SrNo" style="display: none" data-label="lblFirstName">

            <div class="row">
                <div class="col-md-6">
                    <label for="txtFirstName">First Name <span>*</span></label>
                    <input type="text" class="form-control col-sm-6 textbox-control " id="txtFirstName" placeholder="First Name" data-label="lblFirstName">
                </div>
                <div class="col-md-6">
                    <label for="txtLastName">Last Name <span>*</span></label>
                    <input type="text" class="form-control col-sm-6 textbox-control " id="txtLastName" placeholder="Last Name" data-label="lblLastName">
                </div>

            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-md-6">
                    <label for="txtEmail">Email <span>*</span></label>
                    <input type="text" class="form-control textbox-control  email" id="txtEmail" placeholder="Email (User Name)" data-label="lblEmail">
                </div>
                <div class="col-md-6 Editdisable">
                    <label for="txtConfirmEmail">Confirm Email <span>*</span></label>
                    <input type="text" class="form-control confirm  email" id="txtConfirmEmail" data-confirm="txtEmail" placeholder="Confirm Email">
                </div>


            </div>
        </div>

        <div class="form-group">
            <div class="row">
                <div class="col-md-2">
                    <label for="txtOfficialPhoneCode">Code</label>
                    <input type="text" class="form-control url" id="txtOfficialPhoneCode" placeholder="Country Code" value="+91">
                </div>
                <div class="col-md-4">
                    <label for="txtOfficialPhone">Mobile</label>
                    <input type="text" class="form-control" id="txtOfficialPhone" placeholder="Mobile" value="9876543210">
                </div>
                <div class="col-md-6">
                    <label for="txtPassword">Password <span>*</span><span class="fa fa-question-circle text-info" title="Password should contain atleast 1 Uppercase 1 Special character" rel="tooltip"></span> </label>
                    <input type="Password" class="form-control " id="txtPassword" placeholder="Password">
                </div>


            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-md-6 Editdisable">
                    <label for="txtConfirmPassword">Confirm Password <span>*</span></label>
                    <input type="Password" class="form-control confirm " id="txtConfirmPassword" data-confirm="txtPassword" placeholder="Confirm Password">
                </div>
                <div class="col-md-6">
                    <label>Status </label>
                    <select id="drpStatus" class="js-states col-sm-2 form-control Dropdown " style="display: none; width: 100%">
                        <option>Active</option>
                        <option>Inactive</option>
                    </select>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label for="tagSearch">Custom Tags</label>
                        <input type="text" data-role="tagsinput" id="txtTagSearchsum" class="form-control" placeholder="Custom Tags" />
                    </div>
                </div>
            </div>
        </div>
        <br />
        <button id="btnSaveUser" type="submit" class="btn btn-success btnSaveuserclass">
            SAVE</button>
        <button id="btnSaveUser" type="submit" class="btn btn-primary btnSaveuserclass">
            VERIFY USER</button>
        <button id="btnUpdateUser" type="submit" class="btn btn-primary btnSaveuserclass" style="display: none">
            UPDATE</button>
        @*<button id="btnCancelUser" type="button" class="btn btn-green" style="display: none">
                                    DELETE</button>*@
        <button id="btnclearuser" type="button" class="btn btn-warning btnclearuser">
            CLEAR</button>
        <button id="btnQuitUser" type="button" class="btn btn-info quituserbtn">
            QUIT FORM</button>
        <a class="btn btn-refresh" aria-expanded="false" rel="tooltip" title="Refresh Page" onclick="window.location.href=window.location.href;">
            <span class="fa fa-refresh"></span>
        </a>


    </form>
</div>
