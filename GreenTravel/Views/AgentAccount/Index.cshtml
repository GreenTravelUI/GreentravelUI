                confirmButtonText: "Yes, create it!",
                cancelButtonText: "No, cancel it!",
                closeOnConfirm: false,
                closeOnCancel: false
            },
            function (isConfirm) {
                if (isConfirm) {
                    //  window.location.href = "/bookingsfit"
                    swal("Created!", "Your supplier has been created.", "success");

                } else {
