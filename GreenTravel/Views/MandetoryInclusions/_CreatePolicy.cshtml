<div class="col-md-8 full-responsive">
    <form>
        <h3 class="tab9section1">Compulsory Inclusions - <span class="text-danger">Amari Ocean Pattaya - Pattaya</span></h3>
        <div class="form-group">
            <div class="row">
                <div class="col-md-3">
                    <label for="drpCostingSet">Contract</label>
                    <select id="drpCostingTitle" class="js-states form-control" style="display: none; width: 100%">
                         <option>Outtrigger 2015/16 Full Year FIT</option>
                    </select>
                </div>
                <div class="col-md-3">
                    <label for="txtFromDate">Contract ID </label>
                    <input type="text" class="form-control" id="txtContractID" value="0001" placeholder="Contract ID" readonly>
                </div>
                <div class="col-md-3">
                    <label for="drpCostingSet">Costing Set </label>
                    <select id="drpCostingSet" class="js-states form-control" style="display: none; width: 100%">
                        <option selected="selected">Set1</option>
                        <option>Set2</option>
                        <option>Set3</option>
                        <option>Set4</option>
                    </select>
                </div>
                 <div class="col-md-3">
                    <label for="txtFromDate">FIT/Group</label>
                    <input type="text" class="form-control" id="txtContractID" value="FIT" placeholder="FIT/Group" readonly>
                </div>
            </div>
        </div>

        <div class="form-group">
            <div class="row">
                <div class="col-md-6">
                    <label for="drpHotel">Compulsory Type </label>
                    <select id="drpHotel" class="js-states form-control" style="display: none; width: 100%">
                        <option>Gala Dinner</option>
                        <option selected="selected">New Year Eve. Gala Dinner</option>
                    </select>
                </div>
               <div class="col-md-6">
                    <label for="drpRoomCategory">Room Category </label>
                    <select id="drpRoomCategory" class="js-states form-control" multiple="multiple" style="display: none; width: 100%">
                        <option selected="selected">All</option>
                        <option>Standard </option>
                        <option>Delux </option>
                        <option>Supirior</option>
                    </select>
                </div>
               
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                 <div class="col-md-3">
                    <label for="txtFromDate">From Date </label>
                    <input type="text" class="form-control date-picker" id="txtFromDate" value="31/12/2015" placeholder="From Validity">
                </div>
                <div class="col-md-3">
                    <label for="txtTodate">To Date </label>
                    <input type="text" class="form-control date-picker" id="txtTodate" value="31/12/2015" placeholder="To Validity">
                </div>
            </div>
        </div>
        <div class="form-group">
            <label for="lbloperatingweekday">Operating Week Days </label>
            <div class="row">
                <div class="col-md-12">
                    <input type="checkbox" checked>All &nbsp &nbsp
                                            <input type="checkbox" checked>Mon &nbsp &nbsp
                                           <input type="checkbox">Tue &nbsp &nbsp
                                           <input type="checkbox" checked>Wed &nbsp &nbsp
                                           <input type="checkbox" checked>Thu &nbsp &nbsp
                                           <input type="checkbox">Fri &nbsp &nbsp
                                           <input type="checkbox">Sat &nbsp &nbsp 
                                            <input type="checkbox">Sun &nbsp &nbsp
                </div>

            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-md-6">

                    <label for="drpStatus">Status</label>
                    <select id="drpStatus" class="js-states form-control" style="display: none; width: 100%">
                        <option selected="selected">Active </option>
                        <option>Inactive </option>
                    </select>


                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-md-12">
                    <h3>Compulsory Inclusion Detail</h3>
                    <div class="table-responsive">
                        <table style="width: 100%">
                            <thead>
                                <tr>
                                    <th style="width: 35%; padding: 5px">Charge Type</th>
                                    <th style="width: 20%; padding: 5px">Nights</th>
                                    <th style="width: 20%; padding: 5px">Adult Cost</th>
                                    <th style="width: 20%; padding: 5px">Child cost</th>
                                    <th style="width: 5%; padding: 5px"></th>
                                </tr>
                            </thead>
                            <tbody>
                               
                                <tr>
                                    <td style="padding: 5px">
                                        <select id="drpNationalityset" class="js-states form-control" style="display: none; width: 100%">
                                            <option >None</option>
                                            <option>Per Night</option>
                                            <option selected="selected">Per Stay</option>
                                            <option>Min. Nights</option>
                                        </select>
                                    </td>
                                    <td style="padding: 5px">

                                        <input type="text" class="form-control" id="txtCount" value="0" placeholder="Cost">
                                    </td>
                                    <td style="padding: 5px">
                                        <div class="input-group input-append">
                                            <input type="text" class="form-control" id="txtCount" value="5500" placeholder="Cost">
                                            <span class="input-group-addon add-on"><span>THB</span></span>
                                        </div>
                                    </td>
                                    <td style="padding: 5px">
                                        <div class="input-group input-append">
                                            <input type="text" class="form-control" id="txtCount" value="2750" placeholder="Cost">
                                            <span class="input-group-addon add-on"><span>THB</span></span>
                                        </div>
                                    </td>
                                    <td style="padding: 5px">
                                        <a class=" btn btn-success" rel="tooltip" title="Add" style="padding: 3px 10px;"><span class="fa fa-plus"></span></a>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-md-6">
                    <label for="txtRemarks">Remarks </label>
                    <textarea class="form-control" cols="5" id="txtRemarks"></textarea>
                </div>
                <div class="col-md-6">
                    <label for="txtRemarksInternal">Remarks (Internal)</label>
                    <textarea class="form-control" cols="5" id="txtRemarksInternal"></textarea>
                </div>
            </div>
        </div>
        <br />
        <button id="btnSave" type="submit" class="btn btn-success btnSave">
            SAVE</button>
        <button id="btnSave" type="submit" class="btn btn-primary btnSave">
            UPDATE</button>
        <button id="btnCancel" type="submit" class="btn btn-green btnCancel">
            DELETE</button>
        <button id="btnSave" type="submit" class="btn btn-warning btnSave">
            CREATE NEW</button>
        <button id="btnSave" type="submit" class="btn btn-info btnSave">
            QUIT FORM</button>
        <a class="btn btn-refresh" aria-expanded="false" rel="tooltip" title="Refresh Page" onclick="window.location.href=window.location.href;">
            <span class="fa fa-refresh"></span>
        </a>
        <br />

        @* <h3 class="tab9section1 section-headers">Compulsory Details</h3>
        <ul class="nav nav-tabs form-basic-tab form-tabs" role="tablist">
            <li role="presentation" class="active"><a href="#tab6-1" role="tab" data-toggle="tab" data-section="tab1" class="hosting-accord sub-tabbing"><span class="menu-icon fa fa-search"></span>&nbsp;&nbsp;&nbsp;&nbsp;Search Compulsory Details</a></li>
            <li role="presentation"><a href="#tab6-2" role="tab" data-toggle="tab" data-section="tab2" class="basic-accord sub-tabbing"><span class="menu-icon fa fa-ticket "></span>&nbsp;&nbsp;&nbsp;&nbsp;Compulsory Details</a></li>
        </ul>
        <div class="tab-content">
            <div role="tabpanel" class="tab-pane active" id="tab6-1">
                @Html.Partial("_SearchDetail")
            </div>
            <div role="tabpanel" class="tab-pane " id="tab6-2">
                @Html.Partial("_CreateDetail")
            </div>
        </div>*@
    </form>
</div>
