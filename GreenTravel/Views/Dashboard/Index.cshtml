            .date-range span {
                font-size: 10px;
            }

        .btn {
            text-transform: uppercase;
        }

        .dashboard-tab .nav-tabs > li > a {
            border-bottom-width: 0;
        }

        .date-picker-line {
            display: none;
        }

        .dashboard-tab .tab-content {
            border-top-width: 0;
        }

        .nav-tabs {
            border-bottom: 1px solid #ddd;
        }

        .dashboard-tab .nav-tabs > li > a {
            font-size: 12px;
            padding: 10px 20px;
        }
    </style>

}
